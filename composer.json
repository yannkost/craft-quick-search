{
    "name": "yannkost/craft-quick-search",
    "description": "Fast entry search and navigation for the Craft CMS control panel",
    "type": "craft-plugin",
    "version": "1.2.0",
    "license": "proprietary",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "plugin",
        "search",
        "quick-search",
        "navigation",
        "entries",
        "history",
        "favorites"
    ],
    "authors": [
        {
            "name": "Yann Kost",
            "email": "yann.kost@ik.me"
        }
    ],
    "support": {
        "issues": "https://github.com/yannkost/craft-quick-search/issues",
        "source": "https://github.com/yannkost/craft-quick-search"
    },
    "require": {
        "php": ">=8.2",
        "craftcms/cms": "^5.0.0"
    },
    "autoload": {
        "psr-4": {
            "craftcms\\quicksearch\\": "src/"
        }
    },
    "extra": {
        "handle": "quick-search",
        "name": "Quick Search",
        "class": "craftcms\\quicksearch\\Plugin",
        "developer": "Yann Kost",
        "developerUrl": "https://github.com/yannkost",
        "documentationUrl": "https://github.com/yannkost/craft-quick-search/blob/main/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/yannkost/craft-quick-search/main/CHANGELOG.md"
    }
}
